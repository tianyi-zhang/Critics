            <command
                  commandId="edu.utexas.seal.plugins.handler.CriticsSelectContext"
                  label="Select Context Nodes - Method Level"
                  style="push">
            </command>
            <command
                  commandId="edu.utexas.seal.plugins.handler.CriticsFindContext"
                  label="Find Context Group"
                  style="push">
            </command>
            <command
                  commandId="edu.utexas.seal.plugins.handler.CriticsSelectContextClassLevel"
                  label="Select Context Nodes - Class Level"
                  style="push">
